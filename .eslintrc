{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "ignorePatterns": ["artifacts/*", "cache/*", "typechain-types/*", "node_modules/*"],
  "rules": {
    "no-console": 0,
    "prettier/prettier": 2
  }
}
